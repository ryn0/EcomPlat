@model EcomPlat.Data.Models.ConfigSetting

@{
    this.ViewData["Title"] = "Config Setting Details";
}

<h2>@this.ViewData["Title"]</h2>

<div>
    <h4>Config Setting</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            ID
        </dt>
        <dd class="col-sm-10">
            @this.Model.ConfigSettingId
        </dd>
        <dt class="col-sm-2">
            Site Config Setting
        </dt>
        <dd class="col-sm-10">
            @this.Model.SiteConfigSetting
        </dd>
        <dt class="col-sm-2">
            Content
        </dt>
        <dd class="col-sm-10">
            @this.Model.Content
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@this.Model.ConfigSettingId" class="btn btn-warning">Edit</a>
    <a asp-action="Index" class="btn btn-secondary">Back to List</a>
</div>
